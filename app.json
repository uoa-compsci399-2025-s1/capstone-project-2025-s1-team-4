{
  "expo": {
    "name": "MediDex",
    "slug": "MediDex",
    "version": "1.0.0",
    "icon": "./assets/images/splashIconLight.png",
    "orientation": "portrait",
    "scheme": "MediDex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.PSXC",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to scan barcodes"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
